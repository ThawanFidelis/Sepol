<h1>Listing reserves</h1>

<table>
  <tr>
    <th>Usuario</th>
    <th>Inicio</th>
    <th>Termino</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reserves.each do |reserf| %>
  <tr>
    <td><%= reserf.user.name %></td>
    <td><%= reserf.inicio %></td>
    <td><%= reserf.termino %></td>
    <td><%= link_to 'Show', reserf %></td>
    <td><%= link_to 'Edit', edit_reserf_path(reserf) %></td>
    <td><%= link_to 'Destroy', reserf, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

